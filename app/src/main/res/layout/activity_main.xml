<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="de.thorstenhack.moshiretrofittest.MainActivity">

    <Button
        android:id="@+id/button_moshi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Request Moshi" />

    <Button
        android:id="@+id/button_gson"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/button_moshi"
        android:text="Request Gson" />


    <TextView
        android:id="@+id/log"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/button_moshi"
        android:text="Log appears here\n" />


</RelativeLayout>
